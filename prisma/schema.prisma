generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql" // You can change this to "postgresql" or "sqlite"
  url      = env("DATABASE_URL")
}



model Orders {
  id       Int     @id @default(autoincrement())
  name    String 
  phone String 
  house String
  amount String @default("1L")
  createdAt DateTime @default(now())

  @@map("Orders")
}

model Feedback {
  id        Int      @id @default(autoincrement())
  message   String
  createdAt DateTime @default(now())

  @@map("Feedback")
}